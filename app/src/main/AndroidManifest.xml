<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.davidfernandez.airrider">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AirRiderMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DateChooser" />
        <activity android:name=".TimeChooser" />
        <activity android:name=".YourTrips" />
        <activity android:name=".MatchActivity" />
        <activity android:name=".ProfileActivity"></activity>
    </application>

</manifest>